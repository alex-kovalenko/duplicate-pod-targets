ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/FeatureA" "${PODS_CONFIGURATION_BUILD_DIR}/FeatureB" "${PODS_CONFIGURATION_BUILD_DIR}/FoundationFramework"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "${PODS_CONFIGURATION_BUILD_DIR}/FeatureA/FeatureA.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/FeatureB/FeatureB.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/FoundationFramework/FoundationFramework.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "FeatureA" -framework "FeatureB" -framework "FoundationFramework"
OTHER_SWIFT_FLAGS = $(inherited) -D COCOAPODS
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
